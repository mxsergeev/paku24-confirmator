{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
    "no-console": ["warn"],
    "no-unused-vars": ["warn"],
    "camelcase": "off",
    "no-unused-expressions": "off",
    "no-underscore-dangle": "off",
    "no-param-reassign": "off"
  }
}
